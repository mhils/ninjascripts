{
  "name": "Imagify",
  "version": "1.0",
  "incognito":"split",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Imagify"
  },
  "permissions": [
    "tabs", "http://*/*"
  ],
  "web_accessible_resources": [
	"imagify.css"
  ]
}